pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
         stage('Build') {
            steps {
                echo 'Building'
            }
            stage('Deb') {
            steps {
                echo 'Developing'
            }
          stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}}}}
